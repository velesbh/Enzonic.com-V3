# Clerk Authentication
# Get your publishable key from https://dashboard.clerk.com/
VITE_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here

# Get your secret key from https://dashboard.clerk.com/ (for server-side operations)
CLERK_SECRET_KEY=your_clerk_secret_key_here

# OpenAI Compatible API
# Your API key for translation services
VITE_OPENAI_API_KEY=your_openai_api_key_here

# API endpoint URL (optional - defaults to Enzonic's endpoint)
VITE_OPENAI_API_URL=https://ai-api.enzonic.me/api/v1/chat/completions